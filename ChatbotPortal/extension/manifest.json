{
  "author": "Apu Islam, Henry Lo, Jacy Mark, Ritvik Khanna, Yeva Nguyen",
  "version": "1.0",
  "manifest_version": 2,
  "name": "Submit Resources to ChatbotPortal",
  "short_name": "submit-resources",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://127.0.0.1:8000/chatbotportal/app/",
  "description": "This extension helps submitting resources directly to ChatbotPortal",
  "permissions": [
    "cookies",
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_title": "Submit Resources",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "web_accessible_resources": [
    "popup.html"
  ]
}
